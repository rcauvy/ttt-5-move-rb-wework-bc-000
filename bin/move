#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," "," "]
display_board(board)


puts "Where would you like to go 1-9?"
input = gets.strip

input_to_index(input)

raise input.inspect

move(board, index, character)

display_board(board)